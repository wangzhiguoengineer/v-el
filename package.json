{
  "name": "v-ele",
  "description": "Vue封装ElementUI，快捷数据驱动开发。",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "webpack",
    "taobao-registry": "npm config set registry http://registry.npm.taobao.org",
    "public-registry": "npm config set registry https://registry.npmjs.org",
    "release": "npm run public-registry && npm run build && npm publish --access=public && npm run taobao-registry"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wangzhiguoengineer/v-ele.git"
  },
  "keywords": [
    "ElementUI",
    "Vue",
    "Form",
    "Table",
    "VueForm",
    "VueTable"
  ],
  "author": "wangzhiguo.engineer@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangzhiguoengineer/v-ele/issues"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-undef": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
